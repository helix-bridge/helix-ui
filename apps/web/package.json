{
  "name": "@helixbridge/web",
  "description": "HelixBridge Web Interface",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm run dev:mainnet",
    "dev:mainnet": "vite --mode mainnet",
    "dev:testnet": "vite --mode testnet",
    "build": "pnpm run build:mainnet",
    "build:mainnet": "tsc && vite build --mode mainnet && node scripts/generate-sitemap.js",
    "build:testnet": "tsc && vite build --mode testnet && node scripts/generate-sitemap.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest --run --silent",
    "clean": "rm -rf node_modules .turbo dist",
    "preview": "vite preview"
  },
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.10.3",
    "@floating-ui/react": "^0.26.14",
    "@helixbridge/helixconf": "1.1.15",
    "@rainbow-me/rainbowkit": "^1.3.7",
    "@sentry/react": "^8.30.0",
    "@sentry/vite-plugin": "^2.22.4",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "graphql": "^16.8.1",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-jazzicon": "^1.0.4",
    "react-router-dom": "^6.23.1",
    "react-transition-group": "^4.4.5",
    "rxjs": "^7.8.1",
    "sort-by": "^0.0.2",
    "viem": "^1.21.4",
    "wagmi": "^1.4.13"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-transition-group": "^4.4.10",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^2.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-pwa": "^0.20.1",
    "vitest": "^1.6.0",
    "workbox-core": "^7.1.0",
    "workbox-window": "^7.1.0"
  },
  "packageManager": "pnpm@9.4.0"
}
